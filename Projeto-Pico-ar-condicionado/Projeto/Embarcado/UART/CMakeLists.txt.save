uart# Generated Cmake Pico project file

cmake_minimum_required(VERSION 3.13)

set(CMAKE_C_STANDARD 11)
set(CMAKE_CXX_STANDARD 17)

# Initialise pico_sdk from installed location
# (note this can come from environment, CMake cache etc)
set(PICO_SDK_PATH "/home/luis/pi/pico/pico-sdk")

# Pull in Raspberry Pi Pico SDK (must be before project)
include(pico_sdk_import.cmake)

project(Projeto-Pico-ar-condicionado C CXX ASM)

set(PICO_CXX_ENABLE_EXCEPTIONS 1)

# Initialise the Raspberry Pi Pico SDK
pico_sdk_init()

# Add executable. Default name is the project name, version 0.1

add_executable(Projeto-Pico-ar-condicionado Projeto-Pico-ar-condicionado.cpp )

pico_set_program_name(Projeto-Pico-ar-condicionado "Projeto-Pico-ar-condicionado")
pico_set_program_version(Projeto-Pico-ar-condicionado "0.1")

pico_enable_stdio_uart(Projeto-Pico-ar-condicionado 1)
pico_enable_stdio_usb(Projeto-Pico-ar-condicionado 0)

# Add the standard library to the build
target_link_libraries(Projeto-Pico-ar-condicionado pico_stdlib)

# Add any user requested libraries
target_link_libraries(Projeto-Pico-ar-condicionado
        hardware_spi
        hardware_pio
        hardware_clocks
        )

pico_add_extra_outputs(Projeto-Pico-ar-condicionado)

